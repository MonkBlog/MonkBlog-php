{
	"name": "benovermyer/monkblog",
	"description": "The Monk blog engine.",
	"keywords": ["blog", "laravel"],
	"license": "MIT",
	"type": "project",
	"version": "1.0.4",
	"require": {
		"laravel/framework": "4.2.*",
		"graham-campbell/markdown": "~2.0",
		"Nathanmac/Parser": "2.*",
		"kriswallsmith/assetic": "~1.2",
		"rcrowe/twigbridge": "~0.6",
		"twig/extensions": "~1.2.0"
	},
	"require-dev": {
		"way/generators": "~2.0",
		"barryvdh/laravel-ide-helper": "1.*",
		"barryvdh/laravel-debugbar": "1.*"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		],
		"psr-4": {
			"MonkBlog\\": "app/src/MonkBlog/"
	  	},
		"files": [
			"app/src/MonkBlog/helpers.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan ide-helper:generate",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan ide-helper:generate",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
